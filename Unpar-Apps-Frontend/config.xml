<?xml version='1.0' encoding='utf-8'?>
<widget id="com.unpar.unparapps" version="2.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Unpar Apps</name>
    <description>
        Unpar application
    </description>
    <author email="unparapps@gmail.com" href="http://pm.unpar.ac.id">
        Direktorat Jenderal Teknologi Informasi
        Universitas Katolik Parahyangan
        2015 - 2016
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-camera" spec="~2.1.0" />
</widget>
